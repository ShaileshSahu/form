<h1 class="text-center">Fill the database for the whole Delhi Region</h1>
<div class="container">
  <div class="row">
<div class="col-lg-3">
</div>
<div class="col-lg-6">
  <form class="form-horizontal" action="/database">
   <div class="form-group">

     <div class="col-sm-10">
       <input type="text" class="form-control" id="email" placeholder="Enter Doctor/Hospital/Clinic" name="doctor">
     </div>
   </div>
   <div class="form-group">

     <div class="col-sm-10">
       <input type="number" class="form-control" id="pwd" placeholder="Enter phone number" name="phone">
     </div>
   </div>

   <div class="form-group">

     <div class="col-sm-10">
       <b>Address</b>
       <textarea  class="form-control"  placeholder="Address" name="address">
     </textarea></div>
   </div>
   <div class="form-group">

     <div class="col-sm-10">
       <input type="text" class="form-control" id="pwd" placeholder="Enter Timing" name="timing">
     </div>
</div>

<div class="form-group">
     <div class="col-sm-10">
       <input type="text" class="form-control" id="pwd" placeholder="Enter speaciality/year of experience" name="speacility">
     </div>
   </div>

   <div class="form-group">
        <div class="col-sm-10">
          <input type="text" class="form-control" id="pwd" placeholder="Enter Services Provided" name="services">

        </div>
      </div>
      <div class="form-group">
           <div class="col-sm-10">
             <input type="text" class="form-control" id="pwd" placeholder="Enter Area head" name="areahead">

           </div>
         </div>

   <div class="form-group">
     <div class="col-sm-offset-2 col-sm-10">
       <div class="checkbox">
         <label><input type="checkbox" name="remember"> Remember me</label>
       </div>
     </div>
   </div>
   <div class="form-group">
     <div class="col-sm-offset-2 col-sm-10">
       <button type="submit" class="btn btn-default">Submit</button>
     </div>
   </div>
 </form>

</div>
<div class="col-lg-3"></div>

</div>

  </div>

</div>
